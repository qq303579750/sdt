            <method name="beforeOpen"><![CDATA[shjqid = params["shjqid"];
cgddid = params["cgddid"];
jqmc = params["jqmc"];
dgrq = params["dgrq"];
tjjzrq = params["tjjzrq"];
this.queryText = this.queryText.replace("p.rybh=s.rybh","p.rybh=s.rybh where p.createTime<='"+tjjzrq+"'");
this.queryText = this.queryText.replace("from salesinfo"," from salesinfo where shzt!='未通过' and zdlx='点购台' and cgddid_id in("+cgddid+") and jqmc in ('"+jqmc+"') group by rybh");
this.queryText = this.queryText.replace("list","list where ((n_utime<='"+dgrq+"')) and ((n_shjq="+params["shjqid"]+") or (n_jqmc in ('"+jqmc+"'))) group by n_rybh");]]></method>
            <method name="onFetch"><![CDATA[if(dataSetRow["n_zje"]!=null){
vars["zje"]=vars["zje"]+dataSetRow["n_zje"];
}
var isrq = 0;
var starttime = new Date(2000, 1, 1);

if(isrq==0 &amp;&amp; vars["rq"]==""){
	if(dataSetRow["n_dgrq"]>starttime){

	vars["rq"]=dataSetRow["n_dgrq"].getFullYear() + "年" + (dataSetRow["n_dgrq"].getMonth()+1) + "月" + dataSetRow["n_dgrq"].getDate() + "日";
	isrq = 1;
	}
}]]></method>
