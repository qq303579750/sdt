            <xml-property name="queryText"><![CDATA[select * from(select t.l_rybh as n_rybh,t.l_xm as n_xm,t.l_utime as n_utime,t.l_ye as n_ye,t.l_shjq as n_shjq,t.l_zhzt as n_zhzt,t.l_zje as n_zje,t.l_cgddid as n_cgddid,t.l_jqmc as n_jqmc,pu.dgrq as n_dgrq from (select p.rybh as l_rybh,p.xm as l_xm,p.ye as l_ye,p.shjq_id as l_shjq,p.zhzt as l_zhzt,p.updateTime as l_utime,s.zje as l_zje,s.CGDDID_id as l_cgddid,s.jqmc as l_jqmc from personinfo p left join (select rybh,jqmc,CGDDID_id,sum(zje) as zje from salesinfo) s ON p.rybh=s.rybh) t left join purchaseorder pu on t.l_cgddid =pu.id) list

]]></xml-property>
